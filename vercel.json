{
    "version": 2,
    "builds": [
      {
        "src": "mysite/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "staticfiles/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "mysite/wsgi.py"
      }
    ]
  }
  